{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "sql:default",
    "sql:allow-execute",
    "dialog:default",
    "fs:default",
    {
      "identifier": "fs:allow-download-read-recursive"
    },
    {
      "identifier": "fs:allow-download-write-recursive"
    },
    {
      "identifier": "fs:allow-rename"
    }
  ]
}
